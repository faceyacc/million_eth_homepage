{
	"name": "is-equal",
	"version": "1.6.2",
	"description": "Are these two values conceptually equal?",
	"author": "Jordan Harband <ljharb@gmail.com>",
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\"",
		"prepublishOnly": "safe-publish-latest",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"pretest": "npm run --silent lint",
		"test:corejs": "nyc tape test-corejs.js",
		"tests-only": "nyc tape 'test/**/*.js'",
		"test": "npm run tests-only && npm run test:corejs",
		"posttest": "aud --production",
		"lint": "eslint ."
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/inspect-js/is-equal.git"
	},
	"bugs": {
		"url": "https://github.com/inspect-js/is-equal/issues"
	},
	"homepage": "https://github.com/inspect-js/is-equal",
	"keywords": [
		"equal",
		"is",
		"compare",
		"comparison",
		"equality"
	],
	"dependencies": {
		"es-get-iterator": "^1.1.2",
		"functions-have-names": "^1.2.2",
		"has": "^1.0.3",
		"is-arrow-function": "^2.0.3",
		"is-bigint": "^1.0.2",
		"is-boolean-object": "^1.1.1",
		"is-callable": "^1.2.3",
		"is-date-object": "^1.0.4",
		"is-generator-function": "^1.0.9",
		"is-number-object": "^1.0.5",
		"is-regex": "^1.1.3",
		"is-string": "^1.0.6",
		"is-symbol": "^1.0.3",
		"isarray": "^2.0.5",
		"object-inspect": "^1.10.3",
		"object.entries": "^1.1.3",
		"object.getprototypeof": "^1.0.1",
		"which-boxed-primitive": "^1.0.2",
		"which-collection": "^1.0.1"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^17.6.0",
		"aud": "^1.1.5",
		"auto-changelog": "^2.2.1",
		"core-js": "^2.6.5 || ^3.12.0",
		"es-value-fixtures": "^1.2.1",
		"es6-shim": "^0.35.6",
		"eslint": "^7.25.0",
		"foreach": "^2.0.5",
		"has-bigints": "^1.0.1",
		"has-symbols": "^1.0.2",
		"make-arrow-function": "~1.1.0",
		"nyc": "^10.3.2",
		"safe-publish-latest": "^1.1.4",
		"tape": "^5.2.2"
	},
	"testling": {
		"files": "test/native.js",
		"browsers": [
			"iexplore/6.0..latest",
			"firefox/3.0..6.0",
			"firefox/15.0..latest",
			"firefox/nightly",
			"chrome/4.0..10.0",
			"chrome/20.0..latest",
			"chrome/canary",
			"opera/10.0..12.0",
			"opera/15.0..latest",
			"opera/next",
			"safari/4.0..latest",
			"ipad/6.0..latest",
			"iphone/6.0..latest",
			"android-browser/4.2"
		]
	},
	"engines": {
		"node": ">= 0.4"
	},
	"greenkeeper": {
		"ignore": [
			"core-js"
		]
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true
	}
}
